    "queries": [
        {
            "label": "All online devices",
            "query": "SELECT * FROM c WHERE c.status = 'online'"
        },
        {
            "label": "Low battery (<20%)",
            "query": "SELECT c.deviceId, c.location, c.batteryLevel FROM c WHERE c.batteryLevel < 20"
        },
        {
            "label": "Latest reading per device",
            "query": "SELECT c.deviceId, c.temperature, c.status, c.timestamp FROM c WHERE c.timestamp = (SELECT VALUE MAX(c2.timestamp) FROM c c2 WHERE c2.deviceId = c.deviceId)"
        },
        {
            "label": "Devices with rising temp",
            "query": "SELECT DISTINCT c.deviceId FROM c WHERE c.temperature > 23 AND c.timestamp > '2024-01-15T11:00:00Z'"
        },
        {
            "label": "Avg temp by building",
            "query": "SELECT SUBSTRING(c.location, 0, 10) as building, AVG(c.temperature) as avgTemp FROM c GROUP BY SUBSTRING(c.location, 0, 10)"
        },
        {
            "label": "Devices needing attention",
            "query": "SELECT DISTINCT c.deviceId, c.location FROM c WHERE c.status IN ('offline', 'warning') OR c.batteryLevel < 20"
        }
    ]
}