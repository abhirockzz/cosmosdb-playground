    "queries": [
        {
            "label": "High rated movies",
            "query": "SELECT c.title, c.rating FROM c WHERE c.rating >= 8.0 ORDER BY c.rating DESC"
        },
        {
            "label": "Leonardo DiCaprio movies",
            "query": "SELECT c.title, c.year FROM c WHERE EXISTS(SELECT VALUE a FROM a IN c.actors WHERE a.name = 'Leonardo DiCaprio')"
        },
        {
            "label": "Recent movies",
            "query": "SELECT c.title, c.year, c.genres FROM c WHERE c.year >= 2020 ORDER BY c.year DESC"
        },
        {
            "label": "Long movies (runtime)",
            "query": "SELECT c.title, c.runtime FROM c WHERE c.runtime > 150 ORDER BY c.runtime DESC"
        },
        {
            "label": "Average rating by year",
            "query": "SELECT c.year, AVG(c.rating) as avgRating FROM c GROUP BY c.year ORDER BY c.year DESC"
        }
    ]
