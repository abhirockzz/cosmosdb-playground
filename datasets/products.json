    "queries": [
        {
            "label": "In stock items",
            "query": "SELECT c.name, c.price FROM c WHERE c.inStock = true"
        },
        {
            "label": "Electronics under $100",
            "query": "SELECT * FROM c WHERE c.category = 'Electronics' AND c.price < 100"
        },
        {
            "label": "Items with premium tag",
            "query": "SELECT c.name, c.price FROM c WHERE ARRAY_CONTAINS(c.tags, 'premium')"
        },
        {
            "label": "Avg price by category",
            "query": "SELECT c.category, AVG(c.price) as avgPrice FROM c GROUP BY c.category"
        },
        {
            "label": "Highly rated (>4 stars)",
            "query": "SELECT c.name, c.reviews FROM c WHERE EXISTS(SELECT VALUE r FROM r IN c.reviews WHERE r.rating >= 5)"
        }
    ]
